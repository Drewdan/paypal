{
  "name": "drewdan/paypal",
  "description": "A simple Paypal Client for the V2 API",
  "license": "MIT",
  "require": {
    "php": "^7.4",
    "laravel/framework": "^8.0.",
    "netresearch/jsonmapper": "^v4.0.0",
    "guzzlehttp/guzzle": "7.4.x-dev"
  },
  "authors": [
    {
      "name": "Andrew Arscott",
      "email": "andrew@prometheuscomputing.co.uk"
    }
  ],
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Drewdan\\Paypal\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Drewdan\\Paypal\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Drewdan\\Paypal\\PaypalClientServiceProvider"
      ]
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "orchestra/testbench": "^6.0",
    "ext-json": "*"
  },
  "scripts": {
    "test": "phpunit",
  }
}
